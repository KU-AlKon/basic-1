#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

const int MAX = 50;
int N, M;
int arr[MAX][MAX];
int main(void)
{
    int result = 1;
    cin >> N >> M;
    for (int i = 0; i < N; i++)
    {
        string temp;
        cin >> temp;
        for (int j = 0; j < M; j++)
            arr[i][j] = temp[j] - '0';
    }

    for (int i = 0; i < N; i++){
        for (int j = 0; j < M; j++){
            for (int k = 1; k < min(N, M); k++){
                if (arr[i + k][j] == arr[i][j] && arr[i + k][j + k] == arr[i][j] && arr[i][j + k] == arr[i][j] && i + k < N && j + k < M )
                    result = max(k+1, result);
            }
        }
    }
    cout << result * result << endl;
    return 0;
}
